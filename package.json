{
    "name": "emotionally-responsive-ui",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "build": "next build",
        "build:ci": "next build",
        "build:storybook": "storybook build",
        "dev": "PORT=3000 next dev",
        "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,json,css,md}'",
        "format:write": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,md}'",
        "graphql-codegen": "graphql-codegen --config codegen.ts",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "playwright": "playwright",
        "start": "node .next/standalone/server.js",
        "start:ci": "node .next/standalone/server.js",
        "storybook": "storybook dev -p 6006",
        "test": "jest --errorOnDeprecated --detectOpenHandles",
        "test:cov": "jest --coverage --errorOnDeprecated --detectOpenHandles",
        "test:e2e": "playwright test --project chrome --config test/playwright.config.ts",
        "test:e2e:changed": "playwright test --project chrome --config test/playwright.config.ts --only-changed=main",
        "test:e2e:debug": "playwright test --config test/playwright.debug.config.ts",
        "ts": "tsc --noEmit --incremental --preserveWatchOutput --pretty"
    },
    "dependencies": {
        "@apollo/client": "^3.11.4",
