        "next": "latest",
        "next-intl": "^3.17.2",
        "next-logger": "^5.0.0",
        "pino": "^9.3.2",
        "pino-pretty": "^11.2.2",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.52.2",
        "react-html-parser": "^2.0.2",
        "react-redux": "^9.1.2",
        "sass": "^1.77.8",
        "ua-parser-js": "^1.0.38",
        "uuid": "^10.0.0",
        "yup": "^1.4.0"
