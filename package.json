        "@testing-library/jest-dom": "6.4.8",
        "@testing-library/react": "16.0.0",
        "@types/eslint": "^8.56.11",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "9.0.6",
        "@types/node": "22.5.0",
